{
  "manifest_version": 3,
  "name": "Aternos server auto confirm",
  "author":  "Prinoy D'Costa",
  "description": "A basic extension to auto confirm an Aternos minecraft server that is queued up.",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://aternos.org/server/*"],
      "js": ["content.js"]
    }
  ],
    "action": {
        "default_title": "Click Me",
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/aternos_confirm_16.png",
            "48": "icons/aternos_confirm_48.png",
            "128": "icons/aternos_confirm_128.png"
        }
    },
    "icons": {
            "16": "icons/aternos_confirm_16.png",
            "48": "icons/aternos_confirm_48.png",
            "128": "icons/aternos_confirm_128.png"
  },
  "permissions": ["tabs"]
}